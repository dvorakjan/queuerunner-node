{
  "name": "noderunner",
  "version": "1.0.0",
  "description": "Daemon for background runnings jobs from Mongo queues (immediate, planned, history)",
  "main": "built/noderunner.js",
  "scripts": {
    "start": "nodemon",
    "build": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "mongo": "docker run -p 27017:27017 mongo"
  },
  "config": {
    "unsafe-perm": true
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^2.2.23",
    "@types/nconf": "0.0.37",
    "@types/node": "^10.12.11",
    "argv-split": "^2.0.1",
    "body-parser": "^1.19.0",
    "chance": "^1.0.4",
    "cron-parser": "^2.16.3",
    "express": "^4.17.1",
    "is-running": "^2.1.0",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongodb": "^2.0.35",
    "nconf": "^0.7.1",
    "nodemailer": "~1.3.4",
    "nodemailer-smtp-transport": "^2.0.0",
    "right-pad": "^1.0.1",
    "socket.io": "^2.3.0",
    "winston": "^3.1.0",
    "winston-transport": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvorakjan/noderunner.git"
  },
  "author": "Jan Dvorak",
