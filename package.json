  "bugs": {
    "url": "https://github.com/dvorakjan/noderunner/issues"
  },
  "homepage": "https://github.com/dvorakjan/noderunner",
  "devDependencies": {
    "@types/chance": "^1.1.0",
    "@types/is-running": "^2.1.0",
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "prettier": "1.15.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  }
}
